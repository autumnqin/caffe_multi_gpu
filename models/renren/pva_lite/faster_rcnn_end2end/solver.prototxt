train_net: "models/renren/pva_lite/faster_rcnn_end2end/train_no_pretrain.prototxt"
base_lr: 0.01
lr_policy: "step"
gamma: 0.33
stepsize: 200000
display: 20
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "pva_lite_faster_rcnn"
iter_size: 1
